      "name": "Contact Form",
      "description": "Contact form for user inquiries",
      "files": ["contact/views.py", "templates/contact/index.html", "contact/urls.py", "contact/models.py"],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {
          "title": "Contact API",
          "version": "1.0.0",
          "description": "API for handling contact form submissions"
        },
        "paths": {
          "/contact/": {
            "get": {
              "summary": "Get contact form",
              "responses": {
                "200": {
                  "description": "Successful response"
                }
              }
            },
            "post": {
              "summary": "Submit contact form",
              "requestBody": {
                "content": {
                  "application/x-www-form-urlencoded": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "email": {
                          "type": "string"
                        },
                        "message": {
                          "type": "string"
                        }
                      },
                      "required": ["name", "email", "message"]
                    }
                  }
                }
              },
              "responses": {
                "200": {
                  "description": "Successful response"
                }
              }
            }
          }
        }
      }
    }
